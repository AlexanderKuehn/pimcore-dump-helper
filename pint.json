{
  "preset": "laravel",
  "rules": {
    "no_extra_blank_lines": {
      "tokens": [
        "case",
        "continue",
        "curly_brace_block",
        "default",
        "extra",
        "parenthesis_brace_block",
        "square_brace_block",
        "switch",
        "throw"
      ]
    }
  }
}
