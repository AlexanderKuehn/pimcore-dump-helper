{
    "name": "vintagesucks/pimcore-dump-helper",
    "description": "Pimcore database dump helper with focus on dump readability.",
    "keywords": ["pimcore", "database", "dump", "helper", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/vintagesucks",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vintagesucks/pimcore-dump-helper/issues",
        "source": "https://github.com/vintagesucks/pimcore-dump-helper"
    },
    "authors": [
        {
            "name": "Nikolas Evers",
            "email": "hello@nikol.as"
        }
    ],
    "require": {
        "php": "^8.0",
        "druidfi/mysqldump-php": "^1.0",
        "laravel-zero/framework": "^9.2",
        "nunomaduro/termwind": "^1.14",
        "symfony/dotenv": "^6.1"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["pimcore-dump-helper"]
}
